<%- include('../partials/header'); -%>
<div style="margin-top:20vh;" class="container">
	<div class="row">
		<div class="col-9 text-center my-auto card p-5 mx-auto text-white bg-dark">
			<h2 class="text-center mb-4 text-info">Search</h2>  
 <form action="/search" method="POST">  
      <div class="form-group"> <input type="date" name="search[from]" placeholder="Date From" class="form-control input-lg" id="cname" required> </div>  
      <div class="form-group"> <input type="date" name="search[to]" placeholder="Date To" class="form-control h-25" id="pwd" required> </div>  
	 <div class="form-group"> <select name="search[type]" placeholder="Account Title" class="form-control input-lg" id="email" required>
		 <option value="purchase">Purchase</option>
		 <option value="sale">Sale</option>
		 </select>
		 </div>  
       <button type="submit" class="btn btn-info form-control">Search</button> 
			</form>  
			</div>
		</div>
	</div>
<%- include('../partials/footer'); -%>